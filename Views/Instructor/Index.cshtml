@model List<Instractor>

<table class="table table-bordered table-hover" style="text-align:center">
    <tr>
        <td>ID</td>
        <td>Name</td>
        <td>Imag</td>
        <td>Salary</td>
        <td>Address</td>
        <td>DepartmentName</td>
        <td>CourseName</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Imag</td>
            <td>@item.Salary</td>
            <td>@item.Address</td>
            <td>@item.Department.Name</td>
            <td>@item.Course.Name</td>
        </tr>
    }
</table>
<br/>
<a href="/Instructor/Add" class=" btn btn-success">Add new Instructor</a>